  <!-- color customization -->
  <style type="text/css">
     body { background:   #6d443d ; }
     .panel > .panel-heading {
        background-image: none;
        background-color:  #7e5b54 ;
        color: white;
    }
    .panel {
       background-color: #39bcbc ;
       color:  #6d443d ;
   }
  </style>

</head>
<body>
  <div class="container-fluid">

    <div class="row" id="personaldata">
      <div class="col-xs-12 col-md-4" id="personalpic_container">
          <!-- <div class="personalpic_container"> -->
              <img src="pics/radicke_olaf.jpg" height="320"  alt="cover">
          </div>
      <div class="col-xs-12 col-md-8" id="personalpic_container">
          <!-- <div class="personalextra_container"> -->
              <div class="personaldata_container">
                  <ul>
                      <li><b>Name:</b> Olaf Radicke</li>
                      <li><b>Geboren:</b> 12.7.1971 in Berlin</li>
                      <li><b>Anschrift:</b> Alpenstr. 9, 86159 Augsburg</li>
                      <li><b>Telefon:</b> 0176 â€“ 23 187 609</li>
                      <li><b>E-Mail:</b> <a href="mailto:briefkasten@olaf-radicke.de">briefkasten@olaf-radicke.de</a></li>
                      <li><b>Xing:</b> <a href="https://www.xing.com/profile/Olaf_Radicke">https://www.xing.com/profile/Olaf_Radicke</a></li>
                      <li><b>GitHub:</b> <a href="https://github.com/OlafRadicke">https://github.com/OlafRadicke</a></li>
                  </ul>
              </div><!-- class="personaldata_container" -->
          </div><!-- class="col-xs-12 col-md-8" id="personalpic_container" -->
     </div><!-- class="row" id="personaldata" -->
     <br>


        <!--<div class="mainbox">-->
      <div class="row" id="mainbox">
        <div class="col-xs-12 col-md-8" id="timeline_container">
                <div class="col-xs-12" id="timeline-item"><h2>Timeline</h2></div>
          <div class="row" id="timeline-items">

{{ range .Years }}
            <div class="col-xs-12 col-md-6" id="timeline-item">

    {{ range .Jobs }}
                <div class="panel panel-default">
                    <div class="panel-heading">{{ .Heading }}</div>
                    <div class="panel-body">
                        {{ .Body }}
                        <hr>
                        <b>{{ .Date }}</b>
                    </div>
                </div>
    {{ end }}

            </div><!-- end timeline-item -->

            <div class="col-xs-12 col-md-6" id="timeline-item_inverted">

    {{ range .OtherTasks}}
                <div class="panel panel-default">
                      <div class="panel-heading">{{ .Heading }}</div>
                      <div class="panel-body">
                {{ range .Parts}}
                            <p><b>{{ .Key }}</b><br> {{ .Value }}</p>
                {{ end }}
                            <hr>
                            <b>{{ .Date }}</b>
                      </div>
                </div>
    {{ end }}
            </div><!-- end timeline-item_inverted -->


                  <div class="col-xs-12 col-md-12"><h3 id="{{ .Date }}">{{ .Date }}</h3></div>
{{ end}}

              </div> <!--row timeline-items  -->
            </div> <!--class="col-xs-12 col-md-8" id="timeline_container"  -->

        <div class="col-xs-12 col-md-4" id="skills_container">
            <h2>Skills</h2>
{{ range .SkillSections }}
            <div class="panel panel-default">
                  <div class="panel-heading">{{ .Title }}</div>
                  <div class="panel-body">
    {{ range .Items }}

                        <div class="progress">
                          <div class="progress-bar progress-bar-info" role="progressbar"
                          aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:{{ .Score }}%">
                            {{ .Topic }}
                          </div>
                        </div>

{{ end }}
                </div>
            </div>
{{ end }}

            </div> <!-- end skills_container -->
        </div> <!-- class="row" id="mainbox" -->
    </div> <!-- class="container-fluid"> -->
  </body>
  </html>
